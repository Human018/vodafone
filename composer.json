{
    "name": "laravel-notification-channels/vodafone",
    "description": "Send Laravel Notifications via the Vodafone SMS API",
    "homepage": "https://github.com/laravel-notification-channels/vodafone",
    "license": "MIT",
    "authors": [
        {
            "name": "Simon Woodard",
            "email": "design@simonwoodard.com.au",
            "homepage": "https://www.simonwoodard.com.au",
            "role": "Developer"
        }
    ],
    "require": {
        "php": ">=7.2",
        "illuminate/notifications": "~5.5 || ~6.0",
        "illuminate/support": "~5.5 || ~6.0",
        "guzzlehttp/guzzle": "^6.3"
    },
    "require-dev": {
        "mockery/mockery": "^1.0",
        "phpunit/phpunit": "^8.0"
    },
    "autoload": {
        "psr-4": {
            "NotificationChannels\\Vodafone\\": "src"
        }
    },
    "config": {
        "sort-packages": true
    },
    "extra": {
        "laravel": {
            "providers": [
                "NotificationChannels\\Vodafone\\VodafoneServiceProvider"
            ]
        }
    }
}
